{
  "name": "@midra/nco-utils",
  "version": "1.0.6",
  "description": "NCOverlay用のユーティリティライブラリ",
  "author": "Midra <me@midra.me> (https://github.com/Midra429)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Midra429/nco-utils.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "test": "conc bun:test:* --max-processes 1",
    "test:parse": "bun test ./test/parse.test.ts",
    "test:compare": "bun test ./test/compare.test.ts",
    "compile": "tsc --noEmit --skipLibCheck",
    "check": "bun run biome check --write",
    "clean": "rm -rf dist",
    "prebuild": "bun run --shell bun clean",
    "build": "bun run tsdown"
  },
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "exports": {
    "./api": {
      "import": "./dist/api/index.js",
      "types": "./dist/api/index.d.ts"
    },
    "./api/*": {
      "import": "./dist/api/*.js",
      "types": "./dist/api/*.d.ts"
    },
    "./api/services/abema": {
      "import": "./dist/api/services/abema/index.js",
      "types": "./dist/api/services/abema/index.d.ts"
    },
    "./api/services/danime": {
      "import": "./dist/api/services/danime/index.js",
      "types": "./dist/api/services/danime/index.d.ts"
    },
    "./api/services/dmmTv": {
      "import": "./dist/api/services/dmmTv/index.js",
      "types": "./dist/api/services/dmmTv/index.d.ts"
    },
    "./api/services/fod": {
      "import": "./dist/api/services/fod/index.js",
      "types": "./dist/api/services/fod/index.d.ts"
    },
    "./api/services/jikkyo": {
      "import": "./dist/api/services/jikkyo/index.js",
      "types": "./dist/api/services/jikkyo/index.d.ts"
    },
    "./api/services/netflix": {
      "import": "./dist/api/services/netflix/index.js",
      "types": "./dist/api/services/netflix/index.d.ts"
    },
    "./api/services/nhkPlus": {
      "import": "./dist/api/services/nhkPlus/index.js",
      "types": "./dist/api/services/nhkPlus/index.d.ts"
    },
    "./api/services/nicolog": {
      "import": "./dist/api/services/nicolog/index.js",
      "types": "./dist/api/services/nicolog/index.d.ts"
    },
    "./api/services/niconico": {
      "import": "./dist/api/services/niconico/index.js",
      "types": "./dist/api/services/niconico/index.d.ts"
    },
    "./api/services/niconico/v1": {
      "import": "./dist/api/services/niconico/v1/index.js",
      "types": "./dist/api/services/niconico/v1/index.d.ts"
    },
    "./api/services/syobocal": {
      "import": "./dist/api/services/syobocal/index.js",
      "types": "./dist/api/services/syobocal/index.d.ts"
    },
    "./api/services/tver": {
      "import": "./dist/api/services/tver/index.js",
      "types": "./dist/api/services/tver/index.d.ts"
    },
    "./api/services/unext": {
      "import": "./dist/api/services/unext/index.js",
      "types": "./dist/api/services/unext/index.d.ts"
    },
    "./common/*": {
      "import": "./dist/common/*.js",
      "types": "./dist/common/*.d.ts"
    },
    "./compare": {
      "import": "./dist/compare/index.js",
      "types": "./dist/compare/index.d.ts"
    },
    "./parse": {
      "import": "./dist/parse/index.js",
      "types": "./dist/parse/index.d.ts"
    },
    "./parse/*": {
      "import": "./dist/parse/*.js",
      "types": "./dist/parse/*.d.ts"
    },
    "./parse/constants": {
      "import": "./dist/parse/constants/index.js",
      "types": "./dist/parse/constants/index.d.ts"
    },
    "./parse/libs/clean": {
      "import": "./dist/parse/libs/clean/index.js",
      "types": "./dist/parse/libs/clean/index.d.ts"
    },
    "./parse/libs/extract": {
      "import": "./dist/parse/libs/extract/index.js",
      "types": "./dist/parse/libs/extract/index.d.ts"
    },
    "./parse/libs/normalize": {
      "import": "./dist/parse/libs/normalize/index.js",
      "types": "./dist/parse/libs/normalize/index.d.ts"
    },
    "./search": {
      "import": "./dist/search/index.js",
      "types": "./dist/search/index.d.ts"
    },
    "./search/*": {
      "import": "./dist/search/*.js",
      "types": "./dist/search/*.d.ts"
    },
    "./types/*": {
      "import": "./dist/types/*.js",
      "types": "./dist/types/*.d.ts"
    }
  },
  "dependencies": {
    "@geolonia/japanese-numeral": "^1.0.2",
    "fast-xml-parser": "^5.3.2",
    "fastest-levenshtein": "^1.0.16",
    "nanoid": "^5.1.6",
    "nanoid-dictionary": "^5.0.0",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/bun": "^1.3.3",
    "concurrently": "^9.2.1",
    "fast-deep-equal": "^3.1.3",
    "tsdown": "^0.17.0-beta.6",
    "typescript": "^5.9.3",
    "utility-types": "^3.11.0"
  }
}
